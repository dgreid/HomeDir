[user]
	email = dgreid@rivosinc.com
	name = Dylan Reid
[color]
	status = auto
	branch = auto
	pager = true
	interactive = auto
	grep = auto
	diff = auto
	showbranch = auto
[color "diff"]
   new = white bold
[core]
   excludesfile = /home/dgreid/.gitignore
[alias]
   ci = commit
   co = checkout
   st = status
   br = branch
   lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
   lgt = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative HEAD~25..
   rs = remote show
   rsn = remote show -n
   chp = cherry-pick
   shst = show --stat
   rbc = rebase --continue
   rbi = rebase --interactive
   rbs = rebase --skip
   rank = !git shortlog -sn --no-merges
[merge]
	conflictstyle = diff3
	tool = vimdiff
[rerere]
	enabled = 1
[http]
	cookiefile = /home/dylan/.gitcookies
[sendemail]
	smtpserver = smtp.gmail.com
	smtpuser = dylan@rivosinc.com
	smtpencryption = tls
	smtpserverport = 587
[credential "smtp://dylan%40rivosinc.com@smtp.gmail.com%3a587"]
	helper = !pass git-send-email/rivos
